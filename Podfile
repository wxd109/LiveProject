project 'LiveProject.xcodeproj'

# Uncomment this line to define a global platform for your project
platform :ios, '8.0'
target 'LiveProject' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
   use_frameworks!

  # Pods for LiveProject
    pod 'AFNetworking'
    pod 'SDWebImage'
    pod 'MJRefresh'
    pod 'MBProgressHUD'
    pod 'BarrageRenderer'
    pod 'GPUImage'
    pod 'YYModel'
    pod 'Masonry'
    pod 'ReactiveCocoa'

  target 'LiveProjectTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LiveProjectUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
